GRANT SELECT ON FUTURE DYNAMIC TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_READWRITE_ROLE);
GRANT OPERATE ON ALL DYNAMIC TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_READWRITE_ROLE);
GRANT OPERATE ON FUTURE DYNAMIC TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_READWRITE_ROLE);




--Grant Privileges to DATABASE FULL ACCESS ROLE
USE ROLE USERADMIN;
CREATE ROLE IF NOT EXISTS IDENTIFIER($DATABASE_FULL_ROLE);


USE ROLE SECURITYADMIN;
GRANT USAGE ON DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT USAGE ON ALL SCHEMAS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;

GRANT OWNERSHIP ON ALL TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL SEQUENCES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE SEQUENCES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL FILE FORMATS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE FILE FORMATS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL STREAMS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE STREAMS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL PROCEDURES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE PROCEDURES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL FUNCTIONS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE FUNCTIONS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL MATERIALIZED VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE MATERIALIZED VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL TASKS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE TASKS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT OWNERSHIP ON ALL DYNAMIC TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)   COPY CURRENT GRANTS;
--GRANT OWNERSHIP ON FUTURE DYNAMIC TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;

GRANT SELECT ON ALL TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT SELECT ON FUTURE TABLES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT SELECT ON ALL VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT SELECT ON FUTURE VIEWS IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT USAGE ON ALL SEQUENCES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT USAGE ON FUTURE SEQUENCES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT USAGE ON ALL STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT READ ON ALL STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT USAGE ON FUTURE STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
GRANT READ ON FUTURE STAGES IN DATABASE IDENTIFIER($DATABASE) TO ROLE IDENTIFIER($DATABASE_FULL_ROLE)  ;
